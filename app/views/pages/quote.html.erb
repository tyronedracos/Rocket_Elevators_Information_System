		<!-- CART -->
		<section>
			<div class="container">

				<!-- CHECKOUT -->
				<%= form_with(model: @quote, local: true) do |form| %>

					<div class="col-lg-7 col-sm-7">
						<div class="heading-title">
							<h1>Ready for a quote?</h1>
						</div>

						<!---------------------------------------------START--------------------------------------------------------------------------------------------------------------------------------------------------->
						<!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------->
						<!-- BILLING -->
						<fieldset class="mt-60">
							<div class="row">
								<div class="col-md-6">
									<label for="contact_department"> Which Building Type? *</label>
									<select id="building_type" class="form-control pointer" name="department">
										<option id="Non" value="None">--- Select ---</option>
										<option id="Rsl" value="Residential">Residential</option><!-- //11 addR -->
										<option id="Cml" value="Commercial">Commercial</option><!-- //10 -->
										<option id="Crp" value="Corporate">Corporate</option><!-- //9 -->
										<option id="Hyb" value="Hybrid">Hybrid</option><!-- //7 string-->
									</select><br>
									<p><strong>Which Elevator Class? *</strong></p>
									<input type="radio" name="Check" value="standard" onclick="checkRadiovalue()" checked> Standard
									<input type="radio" name="Check" value="premium" onclick="checkRadiovalue()"> Premium
									<input type="radio" name="Check" value="excelium" onclick="checkRadiovalue()"> Excelium
								</div>

							</div>
							<div>
								<!----------------------------------------------residential--------------------------------------------------------------------------------------------------------------------------------------------------->
								<!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------->
								<div id="resiQ">

									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="ApartRes">How Many Appartement? *</label>
											<input id="NbApartRes" name="NbApartRes" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcRes()" onchange="CalcRes()" oninput="validity.valid||(value='')";/>
										</div>

									</div>
									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="FloorRes">How Many Floor? *</label>
											<input id="NbStageRes" name="NbStageRes"  novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcRes()" onchange="CalcRes()" oninput="validity.valid||(value='')"; />
										</div>

									</div>

									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="SsRes">How Many Basement? *</label>
											<input id="NbSsRes" name="NbSsRes" novalidate type="number" novalidate class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcRes()" onchange="CalcRes()" oninput="validity.valid||(value='')"; />
										</div>
									</div>
								</div>

								<!----------------------------------------Com----------------------------------------------------------------------------------------------------------------------------------------------------->
								<!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------->
								<div id="comQ">

									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Separate Businesses? *</label>
											<input id="NbBusiCom" name="NbBusiCom" novalidate novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCom()"
											 onchange="CalcCom()" oninput="validity.valid||(value='')"; />
										</div>

									</div>
									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Floor? *</label>
											<input id="NbStageCom" name="NbStageCom" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCom()"
											 onchange="CalcCom()" oninput="validity.valid||(value='')";/>
										</div>

									</div>

									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Basement? *</label>
											<input id="NbSsCom" name="NbSsCom" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCom()"
											 onchange="CalcCom()" oninput="validity.valid||(value='')";/>
										</div>
									</div>
									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Parking? *</label>
											<input id="NbParkCom" name="NbParkCom" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCom()"
											 onchange="CalcCom()" oninput="validity.valid||(value='')"; />
										</div>

									</div>
									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Lift Cages To Deploy? *</label>
											<input id="NbCageCom" name="NbCageCom" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCom()" onchange="CalcCom()"oninput="validity.valid||(value='')"; />
										</div>

									</div>
								</div>

								<!---------------------------------------------------------------------Corporate------------------------------------------------------------------------------------------------------------------------>
								<!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------->
								<div id="corpQ">

									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many separate tenant companies? *</label>
											<input id="NbBusiCorp" name="NbBusiCorp" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCorp()"
											 onchange="CalcCorp()" oninput="validity.valid||(value='')"; />
										</div>

									</div>
									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Floor? *</label>
											<input id="NbStageCorp" name="NbStageCorp" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCorp()"
											 onchange="CalcCorp()" oninput="validity.valid||(value='')"; />
										</div>

									</div>

									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Basement? *</label>
											<input id="NbSsCorp" name="NbSsCorp" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCorp()"
											 onchange="CalcCorp()" oninput="validity.valid||(value='')";/>
										</div>
									</div>
									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Parking? *</label>
											<input id="NbParkCorp" name="NbParkCorp" type="number" novalidate class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCorp()" onchange="CalcCorp()" oninput="validity.valid||(value='')"; />
										</div>
									</div>
									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="billing_email">What Is The Maximum Number Of Occupants Per Floor? *</label>
											<input id="MaxOccCorp" name="MaxOccCorp" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcCorp()"
											 onchange="CalcCorp()" oninput="validity.valid||(value='')";/>
										</div>
									</div>
								</div>
								<!--------------------hybride-------------------------------------------------------------------------------------------------------------------------------------------------------------------------->
								<!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------->

								<div id="hybQ">

									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Separate Businesses? *</label>
											<input id="NbBusiHyb" name="NbBusiHyb" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcHyb()"
											 onchange="CalcHyb()" oninput="validity.valid||(value='')"; />
										</div>

									</div>
									<div class="row">
										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Floor? *</label>
											<input id="NbStageHyb" name="NbStageHyb" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcHyb()"
											 onchange="CalcHyb()" oninput="validity.valid||(value='')"; />
										</div>

									</div>

									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Basement? *</label>
											<input id="NbSsHyb" name="NbSsHyb" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcHyb()" onchange="CalcHyb()" oninput="validity.valid||(value='')"; />
										</div>
									</div>
									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="billing_email">How Many Parking? *</label>
											<input id="NbParkHyb" name="NbParkHyb" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcHyb()"
											 onchange="CalcHyb()" oninput="validity.valid||(value='')"; />
										</div>
									</div>
									<div class="row">

										<div class="col-md-12 col-sm-6">
											<label for="billing_email">What Is The Maximum Number Of Occupants Per Floor? *</label>
											<input id="MaxOccHyb" name="MaxOccHyb" novalidate type="number" class="form-control novalidate rst" min="0" max="10000" onkeyup="CalcHyb()"
											 onchange="CalcHyb()" oninput="validity.valid||(value='')"; />
										</div>
									</div>
									<div class="row">
										<div class="col-md-4">
											<label for="contact_department">Opening Hours Of The Building? *</label>
											<input id="OppHourHyb" name="OppHourHyb" novalidate type="number" class="form-control novalidate rst" min="1" max="24" onkeyup="CalcHyb()"
											 onchange="CalcHyb()" oninput="validity.valid||(value='')"; />
											
										</div>

									</div>

								</div>

								<!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------->
								<!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------->

						</fieldset>
						<!-- /BILLING -->

						<!-- /SHIPPING -->

					</div>



					<div class="col-lg-5 col-sm-5">
						<div class="heading-title">
							<h2 style="position: relative;text-align:'center';">OUR PRICES</h2>
						</div>


						<!-- Quote -->
						<fieldset id="ccPayment" class="mt-30 hide">

							<div class="toggle-transparent toggle-bordered-full clearfix">
								<div class="toggle active">
									<div class="toggle-content">

						
						
									</fieldset>
						<!-- /CREDIT CARD PAYMENT -->


						<!-- TOTAL / PLACE ORDER -->
						<div class="toggle-transparent toggle-bordered-full clearfix">
							<div class="toggle active">
								<div class="toggle-content">

									<span class="clearfix">
										<span id="totalele" class="float-right">0</span>
                                        <input id="TotalElevators" name="nb_of_column" class="float-right" type="hidden" value="">

										<span class="float-left"><strong>Total Elevators:</strong></span>
									</span>
									<span class="clearfix">
										<span id="perc" class="float-right">10%</span>
										<span class="float-left">Installation Fees:</span>
									</span>

									<hr />

									<span class="clearfix">
										<span id="totalfin" class="float-right fs-20">0.00$</span>
                                        <input id="TotalFinal" name="nb_of_column" lass="float-right fs-20" type="hidden" value="">
										<strong class="float-left"><u>TOTAL:</u></strong>
									</span>

									<hr />
									<input type="text" name="Full Name" class="form-control" placeholder="Full Name" required>
                                    <input type="text" name="Phone Number" class="form-control" placeholder="Phone Number" required>
									<button type="submit"class="btn btn-primary btn-lg btn-block fs-15" >Send Quote Now</button>
									
								</div>
							</div>
						</div>
						<!-- /TOTAL / PLACE ORDER -->
						</form>
						
						</span>
						
					</div>
					
                    
				<% end %>
			</div>
		</section>
